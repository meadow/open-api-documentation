{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Starter Kit",
  "logo": {
    "dark": "/images/logo-white.png",
    "light": "/images/logo-purple.png"
  },
  "favicon": "/images/favicon.svg",
  "colors": {
    "primary": "#8019e4",
    "light": "#63dde8",
    "dark": "#8019e4"
  },
  "layout": "solidSidenav",
  "sidebar": {
    "items": "card"
  },
  "codeBlock": {
    "mode": "auto"
  },
  "navigation": [
    {
      "group": "Using the API",
      "pages": [
        "introduction",
        "authentication",
        "quickstart",
        "development"
      ]
    },
    {
      "group": "Essentials",
      "pages": [
        "essentials/markdown",
        "essentials/code",
        "essentials/images",
        "essentials/settings",
        "essentials/navigation",
        "essentials/reusable-snippets"
      ]
    },
    {
      "group": "API Documentation",
      "pages": [
        "api-reference/introduction"
      ]
    },
    {
      "group": "Endpoint Examples",
      "pages": [
        "api-reference/endpoint/get",
        "api-reference/endpoint/create",
        "api-reference/endpoint/delete"
      ]
    },
    {
      "group": "Endpoints: Orders",
      "pages": [
        "endpoints/orders/get-status",
        "endpoints/orders/post-pricing",
        "endpoints/orders/post-create"
      ]
    }
  ],
  "footerSocials": {
    "github": "https://github.com/meadow"
  },
  "api": {
    "paramFields": {
      "expanded": "all"
    }
  }
}
