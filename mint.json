{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Starter Kit",
  "logo": {
    "dark": "/images/logo-white.png",
    "light": "/images/logo-purple.png"
  },
  "favicon": "/images/favicon.svg",
  "colors": {
    "primary": "#8019e4",
    "light": "#63dde8",
    "dark": "#8019e4"
  },
  "layout": "solidSidenav",
  "sidebar": {
    "items": "card"
  },
  "codeBlock": {
    "mode": "auto"
  },
  "navigation": [
    {
      "group": "Using the API",
      "pages": [
        "introduction",
        "authentication"
      ]
    },
    {
      "group": "Essentials",
      "pages": [
        "essentials/markdown",
        "essentials/code",
        "essentials/images",
        "essentials/settings",
        "essentials/navigation",
        "essentials/reusable-snippets"
      ]
    },
    {
      "group": "Endpoints: Analytics",
      "pages": [
        "endpoints/analytics/get-customers",
        "endpoints/analytics/retrieve-customer",
        "endpoints/analytics/get-orders",
        "endpoints/analytics/get-order-detail",
        "endpoints/analytics/get-order-returns"
      ]
    },
    {
      "group": "Endpoints: Orders",
      "pages": [
        "endpoints/orders/get-status",
        "endpoints/orders/post-pricing",
        "endpoints/orders/post-create"
      ]
    },
    {
      "group": "Endpoints: Products",
      "pages": [
        "endpoints/products/get-menu",
        "endpoints/products/get-tags",
        "endpoints/products/put-update-upc"
      ]
    }
  ],
  "footerSocials": {
    "github": "https://github.com/meadow"
  },
  "api": {
    "paramFields": {
      "expanded": "all"
    }
  }
}
